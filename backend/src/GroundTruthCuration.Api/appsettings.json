{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Datastores": {
    "GroundTruthCuration": {
      "ConnectionString": "Server=tcp:{server_name}.database.windows.net,1433;Initial Catalog=GroundTruthDB;Persist Security Info=False;User ID={your_username@your_domain.com};MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=False;Authentication=\"Active Directory Interactive\";"
    },
    "ManufacturingDataDocDb": {
      "ConnectionString": "AccountEndpoint=https://{account_name}.documents.azure.com:443/;AccountKey={account_key};Database=ManufacturingDataDocDB;",
      "DatabaseName": "{database_name}"
    },
    "ManufacturingDataRelDb": {
      "ConnectionString": "Server=tcp:{server_name}.database.windows.net,1433;Initial Catalog=ManufacturingDataRelDB;Persist Security Info=False;User ID={your_username@your_domain.com};MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=False;Authentication=\"Active Directory Interactive\";"
    }
  },
  "AllowedHosts": "*"
}